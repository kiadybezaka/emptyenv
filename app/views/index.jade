extends _base/layout.jade

block vars

  - title = 'Kiady Bezaka'

block main
  .section-slider

    .section-slider__content

      picture.product-category__img
        source(srcset="images/banner/banner-480.jpg", media="(max-width: 480px)")
        source(srcset="images/banner/banner-768.jpg", media="(max-width: 768px)")
        source(srcset="images/banner/banner-1200.jpg", media="(max-width: 1200px)")
        img(src="images/banner/banner-1920.jpg" , width="1920" , height="500" , alt="banner kiady bezaka" )

    .section-content

      .section-content__portfolio
        img.portfolio__img(src="images/avatar/kiadybezaka.jpg", height="200", width="200" alt="avatar kiay bezaka" )
        p.portfolio__first-name Razafindrabe
        p.portfolio__name Burel Kiady Bezaka

      .section-content__about
        h2.about__title Développeur &amp; Photographe
        p.about__content Tu autem, Fanni, quod mihi tantum tribui dicis quantum ego nec adgnosco nec postulo, facis amice; sed, ut mihi videris, non recte iudicas de Catone; aut enim nemo, quod quidem magis credo, aut si quisquam, ille sapiens fuit. Quo modo, ut alia omittam, mortem filii tulit! memineram Paulum, videram Galum, sed hi in pueris, Cato in perfecto et spectato viro.

  .section-content.mobile-only

    .section-content__about

      h2.about__title Développeur &amp; Photographe
      p.about__content Tu autem, Fanni, quod mihi tantum tribui dicis quantum ego nec adgnosco nec postulo, facis amice; sed, ut mihi videris, non recte iudicas de Catone; aut enim nemo, quod quidem magis credo, aut si quisquam, ille sapiens fuit. Quo modo, ut alia omittam, mortem filii tulit! memineram Paulum, videram Galum, sed hi in pueris, Cato in perfecto et spectato viro.

  .section-album#photographer
    h2.section-album__title Galerie
    p.section-album__description Les photos que j'ai prises
    img.section-album__logo(src="images/logos/logob.png", height="500", width="200" alt="logo kiady bezaka" )
    .section-album__content.desktop-only
      a(href="images/gallery/wedding-renhaut-diary.jpg" data-lightbox="wedding-renhaut-diary" data-title="Wedding - Renhaut & Diary")
        span.hide-only Album image
        img.section-album__img.js-parallax(src="images/gallery/wedding-renhaut-diary.jpg", height="1920", width="800" alt="wedding-renhaut-diary")
    .section-album__content
      .section-album__box-img
        a(href="images/gallery/wedding-eric-felana.jpg" data-lightbox="wedding-renhaut-diary" data-title="Wedding - Eric & Felana")
          span.hide-only Album image
          img.section-album__img(src="images/gallery/wedding-eric-felana.jpg", height="1920", width="800" alt="wedding-eric-felana" )
      .section-album__box-img
        a(href="images/gallery/wedding-renhaut-diary-2.jpg" data-lightbox="wedding-renhaut-diary" data-title="Wedding - Renhaut & Diary")
          span.hide-only Album image
          img.section-album__img(src="images/gallery/wedding-renhaut-diary-2.jpg", height="1920", width="800" alt="wedding-renhaut-diary" )
      .section-album__box-img
        a(href="images/gallery/wedding-rivo-mirantsoa.jpg" data-lightbox="wedding-renhaut-diary" data-title="Wedding - Rivo & Mirantsoa")
          span.hide-only Album image
          img.section-album__img(src="images/gallery/wedding-rivo-mirantsoa.jpg", height="1920", width="800" alt="wedding-rivo-mirantsoa" )
      .section-album__box-img
        a(href="images/gallery/wedding-fabrice-annie.jpg" data-lightbox="wedding-renhaut-diary" data-title="Wedding - Fabrice & Annie")
          span.hide-only Album image
          img.section-album__img(src="images/gallery/wedding-fabrice-annie.jpg", height="1920", width="800" alt="wedding-fabrice-annie" )
      .section-album__box-img
        a(href="images/gallery/wedding-eric-felana-2.jpg" data-lightbox="wedding-renhaut-diary" data-title="Wedding - Eric & Felana")
          span.hide-only Album image
          img.section-album__img(src="images/gallery/wedding-eric-felana-2.jpg", height="1920", width="800" alt="wedding-eric-felana-2" )

  .section-tarif
    .section-tarif__content
      .section-tarif__service
        h2 Mon tarif
        p Tu autem, Fanni, quod mihi tantum tribui dicis quantum ego nec adgnosco nec postulo, facis amice.Sed, ut mihi videris, non recte iudicas de Catone; aut enim nemo, quod quidem magis credo, aut si quisquam, ille sapiens fuit. Quo modo, ut alia omittam, Cato in perfecto et spectato viro.
        p
          |Mon tarif journalier est de
          strong X00€ HT/jours
