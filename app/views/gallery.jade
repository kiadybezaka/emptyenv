            .fun__album-img-title
              p Bruel &amp; Christelle
          
        .section-skill__separator
          p.section-skill__subtitle &nbsp;Portraits&nbsp;

        .fun__album
          a.fun__album-img.overlay(href="http://kiadybezaka.com/portrait-diane" target="_blank" rel="noopener noreferrer")
            img.js-lazy(data-src="images/gallery/overlay/diane.jpg", height="200", width="200" alt="dqvid dias")
            .fun__album-img-title
              p Diane
        
        









