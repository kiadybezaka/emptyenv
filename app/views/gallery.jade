extends _base/layout.jade

block vars

  - title = 'Kiady Bezaka | Galerie'

block main

  .section-other#other
    h2.section-other__title Galerie
    p.section-other__description "Les photos que j'ai prises ..."
    .section-other__content
      .section-other__fun
        .section-skill__separator
          p.section-skill__subtitle &nbsp;Evenements&nbsp;
        

        .fun__album
          a.fun__album-img.overlay(href="http://kiadybezaka.com/wedding-bruel-christelle" target="_blank" rel="noopener noreferrer")
